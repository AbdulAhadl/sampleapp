pipeline{
    agent any
    environment{
        dockertag = getDockerTag()
    }
    stages{
        stage("docker build"){
            steps{
                sh " sudo docker build -t  ahadwaqar/development:${dockertag} ."
            }
        }
    }
}
def getDockerTag(){
    def DockerTag = sh script: 'git rev-parse HEAD',returnstrout:true
    return DockerTag

}